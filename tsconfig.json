{
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "dom",
      "es2019"
    ],
    "jsx": "react-jsx",
    "module": "esnext",
  "moduleResolution": "node",
  "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "react-native": [
        "node_modules/react-native-web"
      ]
    },
    "types": [
      "node",
      "react",
      "react-dom"
    ],
    "baseUrl": "."
  },
  "include": [
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
